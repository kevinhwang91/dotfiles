{
	"Method Definition": {
		"prefix": "fum",
		"body": ["${1|public,private,protected|} ${2:name}($3)$4 {", "\t$0", "}"],
		"description": "Private Method Definition"
	},
	"Property getter": {
		"prefix": "getp",
		"body": ["public get ${1:property}(): ${2:type}$3 {", "\t${4:return $0}", "}"],
		"description": "Property getter"
	},
	"Property setter": {
		"prefix": "setp",
		"body": ["public set ${1:property}(property: ${2:type}) {", "\tthis.$3 = property", "}"],
		"description": "Property setter"
	},
	"Function Statement": {
		"prefix": "fun",
		"body": ["function ${1:name}(${2:params})$3 {", "\t$0", "}"],
		"description": "Function Statement"
	}
}
